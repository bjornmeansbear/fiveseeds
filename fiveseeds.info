; $Id
name        = Five Seeds Farm Custom Theme
description = Five seeds farm theme built off of HTML5_base: A base theme for making HTML5 sites. This theme requires the HTML5 Tools module. http://drupal.org/project/html5_tools.
screenshot  = images/screenshot.png
version     = 1.0
core        = 6.x
engine      = phptemplate
base theme  = html5_base

; REGIONS
; ------------------------------
; Here is a very basic set. Go ahead and add some yourself.
regions[header] = Header
regions[content_top] = Content top
regions[content_bottom] = Content bottom
regions[sidebar_first] = First sidebar
regions[sidebar_second] = Second sidebar
regions[footer_region] = Footer


; CSS
; ------------------------------
stylesheets[all][]   = css/default.css
stylesheets[all][]   = css/html5.css
stylesheets[all][]   = css/layout.css
stylesheets[all][]   = css/style.css
stylesheets[all][]   = css/fiveseeds.css
stylesheets[print][] = css/print.css


; FEATURES AND SETTINGS
; ------------------------------
features[] = logo
features[] = name
features[] = slogan
features[] = mission
features[] = node_user_picture
features[] = comment_user_picture
features[] = search
features[] = favicon
features[] = primary_links
features[] = secondary_links

settings[fiveseeds_modernizr]            = 0
settings[fiveseeds_use_mobile_viewport]  = 0
settings[fiveseeds_mobile_viewport]      = 'width=device-width; initial-scale=1.0; maximum-scale=1.0;'
settings[fiveseeds_include_chrome_frame] = 1
settings[fiveseeds_zen_tabs]              = 0
settings[fiveseeds_wireframe]             = 0
settings[fiveseeds_block_editing]         = 0
settings[fiveseeds_rebuild_registry]      = 0
settings[fiveseeds_breadcrumb]            = yes
settings[fiveseeds_breadcrumb_separator]  = ' Â» '
settings[fiveseeds_breadcrumb_home]       = 1
settings[fiveseeds_breadcrumb_trailing]   = 0
settings[fiveseeds_breadcrumb_title]      = 0


; Information added by drupal.org packaging script on 2010-11-02
version = "6.x"
core = "6.x"
project = "fiveseeds"

